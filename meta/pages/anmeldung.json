{
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowRegister": false,
                  "allowResetPassword": false,
                  "autoComplete": true,
                  "events": {
                    "login": [
                      {
                        "name": "bolSichtbarProgressBar",
                        "type": "set",
                        "value": "true",
                        "valueType": "bool"
                      },
                      {
                        "error": [
                          {
                            "detail": "${result.error.message}",
                            "severity": "error",
                            "summary": "Laden Benutzer fehlerhaft!",
                            "type": "notify"
                          },
                          {
                            "name": "bolSichtbarProgressBar",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "name": "dbSinDarEla.getBenutzers",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "Benutzername eq '${event.Username}'"
                          },
                          {
                            "name": "$count",
                            "value": "true"
                          },
                          {
                            "name": "$expand",
                            "value": "Base"
                          }
                        ],
                        "then": [
                          {
                            "name": "intBenutzerAnzahl",
                            "type": "set",
                            "value": "${result.Count}",
                            "valueType": "int"
                          },
                          {
                            "condition": "intBenutzerAnzahl == 1",
                            "global": true,
                            "name": "globalBenutzer",
                            "type": "set",
                            "value": "${result.Value.AsODataEnumerable().FirstOrDefault()}",
                            "valueType": "SinDarElaVerwaltung.Models.DbSinDarEla.Benutzer"
                          },
                          {
                            "global": false,
                            "name": "bolAnmeldungOK",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          },
                          {
                            "code": "Console.WriteLine(\"1\");",
                            "type": "execute"
                          },
                          {
                            "condition": "intBenutzerAnzahl == 1",
                            "global": false,
                            "name": "bolAnmeldungOK",
                            "type": "set",
                            "value": "${Globals.globalBenutzer.Kennwort} == GetDeterministicHashCode(${event.Password} + ${Globals.globalBenutzer.BaseID})",
                            "valueType": "bool"
                          },
                          {
                            "code": "Console.WriteLine(\"2\");",
                            "type": "execute"
                          },
                          {
                            "code": "await WriteLocalStorage(\"storageBenutzerIDCode\", ${Globals.globalBenutzer.BenutzerIDCode});",
                            "condition": "bolAnmeldungOK == true",
                            "type": "execute"
                          },
                          {
                            "code": "Console.WriteLine(\"3\");",
                            "type": "execute"
                          },
                          {
                            "condition": "bolAnmeldungOK == true",
                            "name": "dsoBenutzerProtokoll",
                            "type": "set",
                            "value": "new SinDarElaVerwaltung.Models.DbSinDarEla.BenutzerProtokoll(){}",
                            "valueType": "SinDarElaVerwaltung.Models.DbSinDarEla.BenutzerProtokoll"
                          },
                          {
                            "code": "dsoBenutzerProtokoll.BenutzerID = Globals.globalBenutzer.BenutzerID;\ndsoBenutzerProtokoll.Art = \"Anmeldung\";\ndsoBenutzerProtokoll.TimeStamp = DateTime.Now;",
                            "condition": "bolAnmeldungOK == true",
                            "type": "execute"
                          },
                          {
                            "condition": "bolAnmeldungOK == true",
                            "name": "dbSinDarEla.createBenutzerProtokoll",
                            "parameters": [
                              {
                                "name": "BenutzerProtokoll",
                                "value": "${dsoBenutzerProtokoll}"
                              }
                            ],
                            "type": "invoke"
                          },
                          {
                            "condition": "bolAnmeldungOK == true",
                            "path": "dashboard",
                            "type": "navigate"
                          },
                          {
                            "condition": "bolAnmeldungOK == false",
                            "detail": "Benutzername oder Kennwort falsch!",
                            "severity": "error",
                            "type": "notify"
                          },
                          {
                            "condition": "bolAnmeldungOK == false",
                            "global": false,
                            "name": "bolSichtbarProgressBar",
                            "type": "set",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "register": []
                  },
                  "loginText": "Anmelden",
                  "name": "login0",
                  "passwordRequired": "Kennwort erfoderlich",
                  "passwordText": "Kennwort",
                  "registerMessageText": "",
                  "registerText": "",
                  "resetPasswordText": "",
                  "type": "login",
                  "userRequired": "Benutzername erforderlich",
                  "userText": "Benutzername",
                  "visible": true
                },
                {
                  "mode": "indeterminate",
                  "name": "progressbar0",
                  "showValue": false,
                  "style": {
                    "marginBottom": "30px",
                    "marginRight": "",
                    "marginTop": "30px"
                  },
                  "type": "progressbar",
                  "value": 100,
                  "visible": "${bolSichtbarProgressBar}"
                }
              ],
              "name": "col0",
              "type": "col"
            }
          ],
          "name": "row0",
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "bolSichtbarProgressBar",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      }
    ]
  },
  "layout": "Login",
  "name": "Anmeldung"
}