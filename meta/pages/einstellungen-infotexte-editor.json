                                  {
                                    "name": "InfotextID",
                                    "value": "${parameters.InfotextID}"
                                  },
                                  {
                                    "name": "InfotexteHtml",
                                    "value": "${dsoInfotexteHTML}"
                                  }
                                ],
                                "then": [
                                  {
                                    "detail": "Infotext gespeichert",
                                    "severity": "success",
                                    "type": "notify"
                                  },
