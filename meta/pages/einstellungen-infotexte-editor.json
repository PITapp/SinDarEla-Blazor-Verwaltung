        "name": "bolEditorAenderungen",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "error": [],
        "name": "dbSinDarEla.getInfotexteHtmlByInfotextID",
        "parameters": [
          {
            "name": "InfotextID",
            "value": "${parameters.InfotextID}"
          }
        ],
        "then": [
          {
            "name": "dsoInfotexteHTML",
            "type": "set",
            "value": "${result}",
            "valueType": "SinDarElaVerwaltung.Models.DbSinDarEla.InfotexteHtml"
          },
          {
            "name": "strTitel",
            "type": "set",
            "value": "${result.Titel}",
            "valueType": "string"
          },
          {
            "name": "strInhalt",
            "type": "set",
            "value": "${result.Inhalt}",
            "valueType": "string"
          }
        ],
        "type": "invoke"
      }
