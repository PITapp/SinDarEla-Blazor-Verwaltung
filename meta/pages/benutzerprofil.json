            "name": "strBildURL",
            "type": "set",
            "value": "${dsoBenutzer.Base.BildURL}",
            "valueType": "string"
          },
          {
            "name": "strBenutzername",
            "type": "set",
            "value": "${dsoBenutzer.Benutzername}",
            "valueType": "string"
          },
          {
            "name": "strInitialen",
