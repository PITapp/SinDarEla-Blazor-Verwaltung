      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "bolEditorAenderungen",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "error": [],
        "name": "dbSinDarEla.getInfotexteHtmlByInfotextID",
        "parameters": [
          {
            "name": "InfotextID",
            "value": "${parameters.InfotextID}"
          }
        ],
        "then": [
          {
            "name": "dsoInfotexteHTML",
            "type": "set",
            "value": "${result}",
            "valueType": "SinDarElaVerwaltung.Models.DbSinDarEla.InfotexteHtml"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "Copy of EinstellungenInfotexteEditor"
