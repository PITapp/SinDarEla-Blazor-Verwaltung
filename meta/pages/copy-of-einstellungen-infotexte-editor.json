                  "buttonStyle": "success",
                  "buttonType": "button",
                  "events": {
                    "click": [
                      {
                        "error": [
                          {
                            "detail": "Infotext konnte nicht gespeichert werden!",
                            "severity": "error",
                            "summary": "",
                            "type": "notify"
                          }
                        ],
                        "name": "dbSinDarEla.updateInfotexteHtml",
                        "parameters": [
                          {
                            "name": "InfotextID",
                            "value": "${parameters.InfotextID}"
                          },
                          {
                            "name": "InfotexteHtml",
                            "value": "${dsoInfotexteHTML}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Infotext gespeichert",
                            "severity": "success",
                            "type": "notify"
                          },
                          {
                            "result": "${result}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "icon": "done",
                  "name": "button0",
                  "text": "Speichern",
                  "type": "button"
                },
                {
                  "buttonStyle": "light",
                  "events": {
                    "click": [
                      {
                        "condition": "this.bolEditorAenderungen == false",
                        "type": "back"
                      },
                      {
                        "condition": "this.bolEditorAenderungen == true",
                        "parameters": [
                          {
                            "name": "strMeldung",
                            "value": "\"Der Text wurde geändert! Soll die Bearbeitung wirklich abgebrochen werden?\""
                          }
                        ],
                        "path": "meldung-ja-nein",
                        "then": [
                          {
                            "condition": "${result} == \"Ja\"",
                            "type": "back"
                          }
                        ],
                        "title": "Text geändert",
                        "type": "popup",
                        "width": 720
                      }
                    ]
                  },
                  "name": "button1",
                  "style": {
                    "marginLeft": "1rem"
                  },
                  "text": "Abbruch",
                  "type": "button"
                }
              ],
